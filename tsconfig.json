{
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowUnreachableCode": false,
    "esModuleInterop": true,
    "isolatedModules": true
  },
  "include": [".ts", ".tsx"],
  "exclude": ["node_modules"]
}
